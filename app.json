{
  "expo": {
    "name": "113MAIC_App",
    "slug": "113MAIC_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",  
    "scheme": "113maicapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "edgeToEdgeEnabled": true
    },

    "web": {
      "bundler": "metro"
    },

    "plugins": [
      "expo-router"
    ],

    "experiments": {
      "typedRoutes": true
    }
  }
}
